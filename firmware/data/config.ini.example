; ConeZ configuration file
; Copy to /config.ini and edit as needed.
; Only include settings you want to override â€” missing keys use compiled defaults.

[wifi]
enabled=on
ssid=RN-ConeZ
password=conezconez

[gps]
origin_lat=40.762173
origin_lon=-119.193672

[lora]
ssid=ConeZ
frequency=431.250
bandwidth=500.0
sf=9
cr=6
preamble=8
tx_power=10
sync_word=0x12
callsign=
; rf_mode=lora             ; "lora" (default) or "fsk"
; fsk_bitrate=4.8          ; kbps (0.6-300)
; fsk_freqdev=5.0          ; kHz (0.6-200)
; fsk_rxbw=156.2           ; kHz (discrete set: 4.8-467)
; fsk_shaping=0            ; 0=none, 1=BT0.3, 2=BT0.5, 3=BT0.7, 4=BT1.0
; fsk_whitening=off        ; data whitening
; fsk_syncword=12AD        ; hex string, 2-16 hex chars (1-8 bytes)
; fsk_crc=2                ; 0=off, 1=1-byte, 2=2-byte

[system]
device_name=
; startup_script=          ; empty = auto-detect (.bas, .c, .wasm)
timezone=-8
auto_dst=on
cone_id=0
cone_group=0
; ntp_server=192.168.1.100   ; local NTP server (default: pool.ntp.org)

[mqtt]
; broker=sewerpipe.local
; port=1883
enabled=on

[led]
count1=50
count2=50
count3=50
count4=50
;color1=0xFF0000
;color2=0x00FF00
;color3=0x0000FF
;color4=0x000000

[debug]
system=on
basic=on
wasm=on
commands=on
gps=off
gps_raw=off
lora=off
lora_raw=off
fsync=off
wifi=off
sensors=off
mqtt=on
other=off
